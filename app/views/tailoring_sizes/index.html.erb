<p id="notice"><%= notice %></p>

<h1>Tailoring Sizes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tailoring_sizes.each do |tailoring_size| %>
      <tr>
        <td><%= link_to 'Show', tailoring_size %></td>
        <td><%= link_to 'Edit', edit_tailoring_size_path(tailoring_size) %></td>
        <td><%= link_to 'Destroy', tailoring_size, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tailoring Size', new_tailoring_size_path %>
