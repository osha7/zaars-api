<p id="notice"><%= notice %></p>

<h1>Listing Shipping Statuses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listing_shipping_statuses.each do |listing_shipping_status| %>
      <tr>
        <td><%= link_to 'Show', listing_shipping_status %></td>
        <td><%= link_to 'Edit', edit_listing_shipping_status_path(listing_shipping_status) %></td>
        <td><%= link_to 'Destroy', listing_shipping_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing Shipping Status', new_listing_shipping_status_path %>
