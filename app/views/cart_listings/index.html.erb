<p id="notice"><%= notice %></p>

<h1>Cart Listings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cart_listings.each do |cart_listing| %>
      <tr>
        <td><%= link_to 'Show', cart_listing %></td>
        <td><%= link_to 'Edit', edit_cart_listing_path(cart_listing) %></td>
        <td><%= link_to 'Destroy', cart_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cart Listing', new_cart_listing_path %>
